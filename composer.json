{
  "name": "taufik-t/uauth-rest-client",
  "description": "OpenID Connect OAuth2 Provider for Laravel Socialite",
  "keywords": [
    "laravel",
    "api",
    "rest"
  ],
  "homepage": "https://github.com/ataufik135/uauth-rest-client",
  "license": "MIT",
  "authors": [
    {
      "name": "Taufik",
      "email": "ataufik135@gmail.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "socialiteproviders/manager": "^4.0",
    "illuminate/http": "^9.0 | ^10.0 | ^11.0 | ^12.0",
    "illuminate/support": "^9.0 | ^10.0 | ^11.0 | ^12.0"
  },
  "autoload": {
    "psr-4": {
      "TaufikT\\UAuthRestClient\\": "src"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "TaufikT\\UAuthRestClient\\ServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}